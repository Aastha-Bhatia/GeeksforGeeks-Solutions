class MyStack{
    private int[] arr;
    private int top;

    public MyStack(){
        arr=new int[100];
        top=-1;
    }
    public void push(int x){
        top++;
        arr[top]=x;
    }
    public int pop(){
        if(top==-1){
            return -1;
        }
        top--;
        return arr[top+1];
    }
}
